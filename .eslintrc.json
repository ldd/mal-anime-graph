{
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["jest", "babel", "import", "prettier"],
  "env": {
    "browser": true,
    "jest/globals": true
  },
  "globals": {
    "Chart": true,
    "debounce": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "babel/semi": 1,
    "default-case": 1,
    "import/extensions": [1, { "mjs": "always" }],
    "import/no-unresolved": 1,
    "import/prefer-default-export": 0,
    "no-continue": 1,
    "no-param-reassign": [2, { "props": false }],
    "no-return-assign": 1,
    "no-unused-vars": 1,
    "no-use-before-define": 1,
    "prettier/prettier": 2,
    "quotes": [2, "double"]
  }
}
